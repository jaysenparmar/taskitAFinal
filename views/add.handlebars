  <h1>Add a Task</h1>
  <div class="w-form">
      
    <form name="email-form" role="form" method="get" action="add">      
         <label class="form_names" for="name">Task Name</label>
      <input class="w-input form_field" type="text" placeholder="Enter the Task Name" name="taskname" data-name="taskname" required="required"></input>
      
        <label class="form_names">User</label>
      <!-- Insert handlebars for users here -->
      <select class="w-select user_drop" name="id" data-name="id" required="required">
        <option value="">Select one...</option>
          {{#each Members}}
        <option name={{name}} value="{{id}} {{name}}">{{name}}</option>
          {{/each}}
      </select>
        
       
      
       <input class="w-button add_submit" type="submit" value="Add" data-wait="Please wait..."></input>
    </form>

    <div class="w-form-done">
